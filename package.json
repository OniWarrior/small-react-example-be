{
  "name": "small-react-app-be",
  "version": "1.0.0",
  "description": "This is the back-end api for the small react app.",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "server": "nodemon index.js",
    "migrate": "knex migrate:latest",
    "rollback": "knex migrate:rollback",
    "mup": "knex migrate:up",
    "mdown": "knex migrate:down",
    "seed": "knex seed:run",
    "resetdb": "npm run rollback && npm run migrate && npm run seed",
    "migrateh": "heroku run knex migrate:latest -a bitcoin-transaction-system-be",
    "rollbackh": "heroku run knex migrate:rollback -a bitcoin-transaction-system-be",
    "databaseh": "heroku pg:psql -a bitcoin-transaction-system-be",
    "seedh": "heroku run knex seed:run -a bitcoin-transaction-system-be",
    "deploy": "git push heroku main",
    "test": "cross-env NODE_ENV=testing jest --watchAll --verbose --runInBand"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.3",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.0",
    "helmet": "^8.0.0",
    "jsonwebtoken": "^9.0.2",
    "jwt-decode": "^4.0.0",
    "knex": "^3.1.0",
    "knex-cleaner": "^1.3.1",
    "pg": "^8.13.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.13",
    "cross-env": "^7.0.3",
    "jest": "^29.7.0",
    "nodemon": "^3.1.7",
    "supertest": "^7.0.0"
  }
}